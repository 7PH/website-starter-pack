{
  "name": "website-starter-pack",
  "private": true,
  "version": "1.0.0",
  "description": "website-starter-pack",
  "main": "index.js",
  "scripts": {
    "start": "docker compose --profile prod up -d --build",
    "debug": "docker compose --profile debug up",
    "dev": "docker compose --profile dev up --build",
    "stop": "docker compose down --remove-orphans",
    "restart": "npm run stop && npm run dev",
    "logs": "docker compose logs -f",
    "build": "docker compose --profile dev build",
    "setup": "bash scripts/_core/setup.sh",
    "db-dump": "bash scripts/_core/db-dump.sh",
    "db-delete": "bash scripts/_core/db-delete.sh",
    "db-restore": "bash scripts/_core/db-restore.sh",
    "db-connect": "bash scripts/_core/db-connect.sh"
  },
  "keywords": [],
  "author": "7PH <b.raymond@protonmail.com>",
  "license": "ISC",
  "devDependencies": {
    "json-schema-to-typescript": "^15.0.3"
  }
}
